{
	"$schema": "vscode://schemas/color-theme",
	"include": "./tokens.hints.json",
	"tokenColors": [
		{
			"scope": ["entity", "entity.name.tag", "constant.regexp"],
			"settings": {
				"foreground": "#3366a6"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.control",
				"constant.language",
				"variable.language",
				"entity.name.selector",
				"entity.other.attribute-name",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#d97000"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#509606"
			}
		},
		{
			"scope": [
				"string",
				"support",
				"markup.inline.raw",
				"keyword.other.unit",
				"meta.structure.dictionary.value",
				"meta.structure.dictionary",
				"meta.structure.array"
			],
			"settings": {
				"foreground": "#009696"
			}
		},
		{
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#960096"
			}
		},
		{
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#76507c"
			}
		},

		{
			"scope": ["markup", "variable"],
			"settings": {} /* reset */
		},
		{
			"scope": ["storage", "source", "entity.name.variable"],
			"settings": {
				"foreground": "#969696"
			}
		},
		{
			"scope": ["markup.bold", "markup.italic"],
			"settings": {
				"foreground": "#33cc33"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#76507c"
			}
		},
		{
			"scope": [
				"storage.type",
				"storage.modifier",
				"meta.structure.dictionary.key.python",
				"support.function.git-rebase"
			],
			"settings": {
				"foreground": "#3366a6"
			}
		},
		{
			"scope": ["constant.sha.git-rebase"],
			"settings": {
				"foreground": "#e0b040"
			}
		},
		{
			"scope": [
				"punctuation.definition.tag",
				"punctuation.section.embedded.begin.metatag.php",
				"punctuation.section.embedded.end.metatag.php"
			],
			"settings": {
				"foreground": "#cc0000"
			}
		}
	]
}
